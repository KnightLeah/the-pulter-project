                                          </xsl:element>
                                          <xsl:text> </xsl:text>
                                        </xsl:when>
                                        <xsl:otherwise>
                                          <xsl:element name="span">
                                            <xsl:attribute name="class">
                                              <xsl:value-of select="'who'"/>
                                            </xsl:attribute>
                                            <xsl:value-of select="."/>
                                          </xsl:element>
                                        </xsl:otherwise>
                                      </xsl:choose>
                                    </xsl:for-each>
                                  </xsl:element>
                                </xsl:element>
                              </xsl:element>
                            </xsl:for-each>
                          </xsl:element>
                        </xsl:when>
                    </xsl:choose>
                    </div>
                    <xsl:if test="$hasCurations">
