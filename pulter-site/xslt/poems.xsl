  <xsl:template match="pp:title">
      <xsl:param name="seoTitle"/>
      <xsl:value-of select="concat(', &quot;title&quot;: ', '&quot;', text() ,'&quot;')"/>
      <xsl:variable name="seoOut">
          <xsl:choose>
              <xsl:when test="$seoTitle">
                  <xsl:value-of select="$seoTitle"/>
              </xsl:when>
              <xsl:otherwise>
                  <xsl:value-of select="text()"/>
              </xsl:otherwise>
          </xsl:choose>
      </xsl:variable>
      <xsl:value-of select="concat(', &quot;seo&quot;: ', '&quot;', $seoOut ,'&quot;')"/>
  </xsl:template>

